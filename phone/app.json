{
  "expo": {
    "name": "SORA+",
    "slug": "phone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/public/logo.png",
    "scheme": "phone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/public/ios-icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.eponamidona.phone",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "アプリ使用中に現在地を取得します"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/public/android-icon.png",
        "backgroundImage": "./assets/images/public/android-bg.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.eponamidona.phone",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDfOnn447wDC1LPOOlrPhMlLzZBHY-ynbo"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e105697b-a8e3-43b7-824f-6f75aa416225"
      }
    }
  }
}
